{
  "name": "PackMate",
  "version": "1.0.0",
  "description": "AI-Powered Mixed Reality Packing Assistant for Meta Quest - Meta Horizon Start Competition",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "build:android": "cd android && ./gradlew assembleRelease",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build:apk": "npm run bundle:android && cd android && .\\gradlew assembleRelease && cd .. && .\\copy-apk.bat",
    "clean": "cd android && ./gradlew clean && cd .. && rm -rf node_modules && npm install"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.82.1",
    "zustand": "^4.4.0",
    "react-native-permissions": "^3.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.82.1",
    "@react-native/eslint-config": "0.82.1",
    "@react-native/metro-config": "0.82.1",
    "@react-native/typescript-config": "0.82.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "react-native",
    "meta-quest",
    "mixed-reality",
    "spatial-sdk",
    "hand-tracking",
    "packing-list",
    "travel-app"
  ],
  "author": "Omar Armesto <maorarmesto@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Oarmestos/PackMate.git"
  },
  "bugs": {
    "url": "https://github.com/Oarmestos/PackMate/issues"
  },
  "homepage": "https://github.com/Oarmestos/PackMate#readme"
}